<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <Authors>Vendr, Outfield Digital Ltd</Authors>
        <Company>Outfield Digital Ltd</Company>
        <Copyright>Outfield Digital Ltd</Copyright>
        <Description>PayPal payment provider for Vendr, the eCommerce package for Umbraco</Description>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://vendr.net</PackageProjectUrl>
        <PackageIcon>logo-128.png</PackageIcon>
        <RepositoryUrl>https://github.com/vendrhub/vendr-payment-provider-paypal</RepositoryUrl>
        <PackageTags>umbraco, vendr, ecommerce, paypal</PackageTags>
        <RepositoryType>git</RepositoryType>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Flurl.Http" Version="3.2.4" />
        <PackageReference Include="Vendr.Core" Version="3.0.7" />
        <PackageReference Include="Microsoft.AspNet.WebApi.Client" Version="5.2.9" />
    </ItemGroup>

    <ItemGroup>
        <None Include="..\..\assets\logo-128.png">
            <Pack>True</Pack>
            <PackagePath></PackagePath>
        </None>
    </ItemGroup>

</Project>
